    <STIG_DATA>
      <VULN_ATTRIBUTE>Group_Title</VULN_ATTRIBUTE>
      <ATTRIBUTE_DATA>System Recovery Backups</ATTRIBUTE_DATA>
    </STIG_DATA>
    <STIG_DATA>
      <VULN_ATTRIBUTE>Rule_ID</VULN_ATTRIBUTE>
      <ATTRIBUTE_DATA>SV-32245r1_rule</ATTRIBUTE_DATA>
    </STIG_DATA>
    <STIG_DATA>
      <VULN_ATTRIBUTE>Rule_Ver</VULN_ATTRIBUTE>
