    <STIG_DATA>
      <VULN_ATTRIBUTE>Group_Title</VULN_ATTRIBUTE>
      <ATTRIBUTE_DATA>Display Shutdown Button</ATTRIBUTE_DATA>
    </STIG_DATA>
    <STIG_DATA>
      <VULN_ATTRIBUTE>Rule_ID</VULN_ATTRIBUTE>
      <ATTRIBUTE_DATA>SV-32280r1_rule</ATTRIBUTE_DATA>
    </STIG_DATA>
